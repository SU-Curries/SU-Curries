{
  "name": "frontend_new",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-helmet-async": "^1.0.3",
    "autoprefixer": "^10.4.14",
    "html-webpack-plugin": "^5.5.3",
    "jest-axe": "^8.0.0",
    "jest-extended": "^4.0.2",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "msw": "^1.3.5",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "ajv-keywords": "^3.5.2",
    "axios": "^1.4.0",
    "eslint-config-prettier": "^8.10.0",
    "framer-motion": "^10.15.0",
    "google-auth-library": "^10.2.0",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^13.0.3",
    "react-intersection-observer": "^9.5.2",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:unit": "jest --testPathPattern=src/tests/components",
    "test:integration": "jest --testPathPattern=src/tests/integration",
    "test:services": "jest --testPathPattern=src/tests/services",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:performance": "jest --testPathPattern=performance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:update-snapshots": "jest --updateSnapshot",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/package.json"
      ],
      "url": "https://json.schemastore.org/package.json"
    }
  ]
}
