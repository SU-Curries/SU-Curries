# SU Curries Application Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_API_TIMEOUT=10000

# Demo Credentials (Development Only - Remove in Production)
REACT_APP_DEMO_ADMIN_EMAIL=admin@sucurries.com
REACT_APP_DEMO_ADMIN_PASSWORD=admin123
REACT_APP_DEMO_USER_EMAIL=user@sucurries.com
REACT_APP_DEMO_USER_PASSWORD=user123

# Security Configuration
REACT_APP_ENABLE_DEMO_MODE=true
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_CSRF_TOKEN_EXPIRY=86400000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_OFFLINE_MODE=false

# Third-party Services
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id_here
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# Email Service
REACT_APP_EMAIL_SERVICE_URL=https://api.emailservice.com
REACT_APP_EMAIL_API_KEY=your_email_api_key_here

# Monitoring & Logging
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_LOG_LEVEL=info

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_DURATION=300000

# Security Headers
REACT_APP_CONTENT_SECURITY_POLICY=default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline';